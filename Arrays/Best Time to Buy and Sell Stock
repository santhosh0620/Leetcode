class Solution {
    public int maxProfit(int[] prices) {
       int maxprofit = 0;
       int minp = Integer.MAX_VALUE;
       for(int i=0;i<prices.length;i++){
        minp = Math.min(minp,prices[i]);
        maxprofit = Math.max(maxprofit,prices[i]-minp);
       }
       return maxprofit;
    }
}
